{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm run vercel-install",
  "framework": "nextjs",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://$BACKEND_URL/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://finguardai-backend.onrender.com"
  },
  "github": {
    "enabled": true,
    "silent": true
  }
}
